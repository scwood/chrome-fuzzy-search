{
  "manifest_version": 2,
  "name": "Fuzzy Tab Finder",
  "version": "1.0.1",
  "description": "Navigate between open tabs quickly",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+P"
      },
      "description": "Opens index.html"
    }
  }
}
